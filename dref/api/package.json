    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~3.1.0",
    "express": "~4.16.0",
    "express-validator": "^5.2.0",
    "http-errors": "~1.7.0",
    "mongoose": "^5.2.11",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.7",
    "pug": "^2.0.3",
    "request": "^2.87.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^7.0.0",
